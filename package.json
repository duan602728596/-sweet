{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea-esm start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --ext .js,.jsx,.vue --fix",
    "update-version": "milktea-esm update",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@sweet-milktea/server": "^4.2.0",
    "@sweet-milktea/server-log": "^4.0.0",
    "@vue/server-renderer": "^3.2.37",
    "@vueuse/head": "^2.0.0",
    "ant-design-vue": "^4.0.7",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "source-map-support": "^0.5.21",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@sweet-milktea/milktea": "^4.2.0",
    "@sweet-milktea/util-tools": "^4.0.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.3.10",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "rimraf": "^4.4.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.3.1",
    "vue-svg-loader": "^0.17.0-beta.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
