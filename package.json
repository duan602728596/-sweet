{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea-esm start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --ext .js,.jsx,.vue --fix",
    "update-version": "milktea-esm update",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@sweet-milktea/milktea-cli": "^3.0.1",
    "@sweet-milktea/server": "^3.0.0",
    "@sweet-milktea/server-log": "^3.0.0",
    "@vue/server-renderer": "^3.1.1",
    "@vueuse/head": "^0.6.0",
    "ant-design-vue": "^2.2.0-beta.3",
    "classnames": "^2.3.1",
    "indexof": "^0.0.1",
    "lodash-es": "^4.17.21",
    "source-map-support": "^0.5.19",
    "vue": "^3.1.1",
    "vue-router": "^4.0.9",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.5",
    "@sweet-milktea/milktea": "^3.0.3",
    "@sweet-milktea/util-tools": "^3.0.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.1",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-vue": "^7.11.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^16.2.0",
    "vue-svg-loader": "^0.17.0-beta.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
