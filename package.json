{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea-esm start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --ext .js,.jsx,.vue --fix",
    "update-version": "milktea-esm update",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "pm": "yarn@^3.2.1"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@sweet-milktea/server": "^4.0.0",
    "@sweet-milktea/server-log": "^4.0.0",
    "@vue/server-renderer": "^3.2.45",
    "@vueuse/head": "^1.0.16",
    "ant-design-vue": "^3.2.15",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.24",
    "source-map-support": "^0.5.21",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@sweet-milktea/milktea": "^4.0.0",
    "@sweet-milktea/util-tools": "^4.1.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.45",
    "babel-plugin-import": "^1.13.5",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^17.0.1",
    "vue-svg-loader": "^0.17.0-beta.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
