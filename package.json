{
  "name": "@sweet/vue-demo",
  "scripts": {
    "dll": "milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea server --serverRender",
    "server:pm2": "pm2 start runServer.js",
    "eslint": "eslint ./ --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet-milktea/milktea-cli": "^0.3.0",
    "@sweet-milktea/server": "^0.3.1",
    "@sweet-milktea/server-log": "^0.3.0",
    "node-cmd": "^3.0.0",
    "pm2": "^3.5.1"
  },
  "devDependencies": {
    "@jnields/vue-helmet": "^3.0.0",
    "@sweet-milktea/milktea": "^0.3.0",
    "@sweet-milktea/util-tools": "^0.3.0",
    "ant-design-vue": "^1.3.10",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
