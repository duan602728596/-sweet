{
  "name": "@sweet/vue-demo",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./build",
    "server": "milktea server",
    "server:pm2": "pm2 start runServer.js",
    "eslint": "eslint ./src --fix",
    "flow:check": "flow check",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet/milktea-cli": "github:duan602728596/sweet#build-milktea-cli",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "node-cmd": "^3.0.0",
    "pm2": "^3.3.1"
  },
  "devDependencies": {
    "@jnields/vue-helmet": "^3.0.0-rc.1",
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "ant-design-vue": "^1.3.4",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "rimraf": "^2.6.3",
    "vue": "^2.6.6",
    "vue-class-component": "^7.0.1",
    "vue-router": "^3.0.2",
    "vue-svg-loader": "^0.12.0",
    "vuex": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
