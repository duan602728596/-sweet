{
  "name": "@sweet/react-demo",
  "scripts": {
    "dll": "milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea server --serverRender",
    "server:pm2": "pm2 start runServer.js",
    "eslint": "eslint ./ --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet-milktea/milktea-cli": "^0.3.5",
    "@sweet-milktea/server": "^0.3.6",
    "@sweet-milktea/server-log": "^0.3.0",
    "node-cmd": "^3.0.0",
    "pm2": "^3.5.1"
  },
  "devDependencies": {
    "@sweet-milktea/milktea": "^0.3.4",
    "@sweet-milktea/util-tools": "^0.3.6",
    "antd": "^3.20.1",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-import": "^1.12.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "immutable": "^4.0.0-rc.12",
    "lint-staged": "^9.1.0",
    "prop-types": "^15.7.2",
    "rc-queue-anim": "^1.6.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
