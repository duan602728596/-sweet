{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --ext .js,.jsx,.vue --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "updateVersion": "milktea update"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@sweet-milktea/milktea-cli": "^2.0.0",
    "@sweet-milktea/server": "^2.0.1",
    "@sweet-milktea/server-log": "^0.6.6",
    "@vue/server-renderer": "^3.0.5",
    "@vueuse/head": "^0.2.3",
    "ant-design-vue": "^2.0.0-rc.9",
    "classnames": "^2.2.6",
    "indexof": "^0.0.1",
    "lodash-es": "^4.17.20",
    "source-map-support": "^0.5.19",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.13",
    "@sweet-milktea/milktea": "^2.0.0",
    "@sweet-milktea/util-tools": "^0.16.4",
    "@vue/babel-plugin-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "es-module-path-case-check": "^1.5.0",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^7.4.1",
    "vue-loader": "^16.1.2",
    "vue-svg-loader": "^0.17.0-beta.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
