{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet-milktea/milktea-cli": "^0.14.0",
    "@sweet-milktea/server": "^0.15.0",
    "@sweet-milktea/server-log": "^0.6.6",
    "@vue/composition-api": "^0.5.0",
    "ant-design-vue": "^1.5.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "indexof": "^0.0.1",
    "lodash-es": "^4.17.15",
    "source-map-support": "^0.5.16",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.3",
    "vue-router": "^3.1.6",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@sweet-milktea/milktea": "^0.15.0",
    "@sweet-milktea/util-tools": "^0.15.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "es-module-path-case-check": "^1.5.0",
    "eslint": "^7.0.0-alpha.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^7.0.0",
    "vue-loader": "^15.9.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
