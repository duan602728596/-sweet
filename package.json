{
  "name": "@sweet/react-demo",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./build",
    "server": "milktea server",
    "server:pm2": "pm2 start runServer.js",
    "eslint": "eslint ./ --fix",
    "case-check": "case-check --dir=\"./\" --ext=\"js,sass,css,jpg,jpeg,png,gif,webp\"",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet/milktea-cli": "github:duan602728596/sweet#build-milktea-cli",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "node-cmd": "^3.0.0",
    "pm2": "^3.4.0"
  },
  "devDependencies": {
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "antd": "^3.15.2",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.0.7",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "es-module-path-case-check": "^0.0.4",
    "eslint": "^5.15.3",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "immutable": "^4.0.0-rc.12",
    "lint-staged": "^8.1.5",
    "prop-types": "^15.7.2",
    "rc-queue-anim": "^1.6.12",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
