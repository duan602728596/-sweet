{
  "name": "@sweet-milktea/react-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development NODE_OPTIONS=\"--max_old_space_size=4096\" milktea-esm start --server --serverRender --socket=ws",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --fix",
    "update-version": "milktea-esm update",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.4.1",
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@reduxjs/toolkit": "^1.9.2",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@sweet-milktea/server": "^4.0.0",
    "@sweet-milktea/server-log": "^4.0.0",
    "antd": "^5.2.1",
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "reselect": "^4.1.7",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@sweet-milktea/milktea": "^4.0.0",
    "@sweet-milktea/util-tools": "^4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "rimraf": "^4.1.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
