{
  "name": "@sweet-milktea/react-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development NODE_OPTIONS=\"--max_old_space_size=4096\" milktea-esm start --server --serverRender --socket=ws",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --fix",
    "update-version": "milktea-esm update",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.1.0",
  "engines": {
    "pm": "yarn@^3.1.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.7.0-beta.1",
    "@sweet-milktea/milktea-cli": "^3.1.1",
    "@sweet-milktea/server": "^3.1.3",
    "@sweet-milktea/server-log": "^3.0.0",
    "antd": "^4.17.1",
    "classnames": "^2.3.1",
    "history": "^5.1.0",
    "prop-types": "^15.7.2",
    "react": "beta",
    "react-dom": "beta",
    "react-helmet": "^6.1.0",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^8.0.0-beta.1",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "reselect": "^4.1.4",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@sweet-milktea/milktea": "^3.1.7",
    "@sweet-milktea/util-tools": "^3.1.6",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
