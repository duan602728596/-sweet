{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea-esm start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --ext .js,.jsx,.vue --fix",
    "update-version": "milktea-esm update",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.1.0",
  "engines": {
    "pm": "yarn@^3.1.0"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@sweet-milktea/milktea-cli": "^3.1.1",
    "@sweet-milktea/server": "^3.1.3",
    "@sweet-milktea/server-log": "^3.0.0",
    "@vue/server-renderer": "^3.2.29",
    "@vueuse/head": "^0.7.5",
    "ant-design-vue": "^3.0.0-beta.8",
    "classnames": "^2.3.1",
    "indexof": "^0.0.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.9",
    "source-map-support": "^0.5.21",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@sweet-milktea/milktea": "^3.1.7",
    "@sweet-milktea/util-tools": "^3.1.6",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.29",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^8.2.0",
    "vue-loader": "^17.0.0",
    "vue-svg-loader": "^0.17.0-beta.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
