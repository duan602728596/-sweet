{
  "name": "@sweet-milktea/vue-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea server --serverRender",
    "server:pm2": "pm2 start runServer.js",
    "eslint": "eslint ./ --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet-milktea/milktea-cli": "^0.10.0",
    "@sweet-milktea/server": "^0.11.1",
    "@sweet-milktea/server-log": "^0.6.6",
    "ant-design-vue": "^1.4.10",
    "axios": "^0.19.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "indexof": "^0.0.1",
    "lodash-es": "^4.17.15",
    "node-cmd": "^3.0.0",
    "pm2": "^4.2.1",
    "source-map-support": "^0.5.16",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-meta": "^2.3.2",
    "vue-router": "^3.1.5",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@sweet-milktea/milktea": "^0.11.1",
    "@sweet-milktea/util-tools": "^0.9.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.2",
    "rimraf": "^3.0.0",
    "vue-eslint-parser": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
