{
  "name": "@sweet-milktea/react-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development NODE_OPTIONS=\"--max_old_space_size=4096\" milktea-esm start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "eslint ./ --fix",
    "update-version": "milktea-esm update",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@reduxjs/toolkit": "^1.6.0",
    "@sweet-milktea/milktea-cli": "^3.0.1",
    "@sweet-milktea/server": "^3.0.0",
    "@sweet-milktea/server-log": "^3.0.0",
    "antd": "^4.16.3",
    "classnames": "^2.3.1",
    "history": "^5.0.0",
    "indexof": "^0.0.1",
    "prop-types": "^15.7.2",
    "react": "next",
    "react-dom": "next",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "reselect": "^4.0.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.5",
    "@sweet-milktea/milktea": "^3.0.2",
    "@sweet-milktea/util-tools": "^3.0.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
