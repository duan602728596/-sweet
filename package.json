{
  "name": "@sweet/vue-demo",
  "scripts": {
    "dll": "milktea dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development milktea start --server --serverRender",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea build --serverRender",
    "clean": "rimraf ./build",
    "clean:server": "rimraf ./buildServer",
    "server": "milktea server --serverRender",
    "server:pm2": "pm2 start runServer.js",
    "eslint": "eslint ./ --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@sweet/milktea-cli": "github:duan602728596/sweet#build-milktea-cli",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "node-cmd": "^3.0.0",
    "pm2": "^3.5.0"
  },
  "devDependencies": {
    "@jnields/vue-helmet": "^3.0.0",
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "ant-design-vue": "^1.3.8",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
