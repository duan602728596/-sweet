{
  "name": "@sweet-milktea/react-demo",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && npm run clean:server && cross-env NODE_ENV=development NODE_OPTIONS=\"--max_old_space_size=4096\" milktea-esm start --server --serverRender --socket=ws",
    "build": "npm run clean && npm run clean:server && cross-env NODE_ENV=production milktea-esm build --serverRender",
    "clean": "rimraf ./dist",
    "clean:server": "rimraf ./dist-server",
    "server": "milktea-esm server --serverRender --redirectToHttps",
    "eslint": "cross-env COMMITLINT=1 eslint ./ --fix --cache --cache-location .eslintcache --no-warn-ignored",
    "eslint:no-fix": "cross-env COMMITLINT=1 eslint ./ --cache --cache-location .eslintcache --no-warn-ignored",
    "update-version": "milktea-esm update",
    "postinstall": "husky install && node postInstall.mjs"
  },
  "packageManager": "yarn@4.4.0",
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@sweet-milktea/server": "^4.3.1",
    "@sweet-milktea/server-log": "^4.0.0",
    "antd": "^5.20.1",
    "classnames": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "rc",
    "react-dom": "rc",
    "react-helmet": "^6.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "reselect": "^5.1.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@eslint/compat": "^1.1.1",
    "@sweet-milktea/milktea": "^4.3.0",
    "@sweet-milktea/util-tools": "^4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "rimraf": "^6.0.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs}": "cross-env COMMITLINT=1 eslint --cache --cache-location .eslintcache --no-warn-ignored"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
