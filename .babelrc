{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["node 6"]
        },
        "debug": false,
        "modules": "commonjs",
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-do-expressions",
    "@babel/plugin-proposal-optional-chaining"
  ]
}