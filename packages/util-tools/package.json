{
  "name": "@sweet-milktea/util-tools",
  "version": "0.6.6",
  "description": "通用工具函数",
  "homepage": "https://github.com/duan602728596/sweet",
  "repository": {
    "type": "git",
    "url": "git@github.com:duan602728596/sweet.git"
  },
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development tsc --watch",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:test && cross-env NODE_ENV=test mocha tests/test.js --timeout 180000",
    "clean:test": "npm run clean:webp && npm run clean:output && npm run clean:icns",
    "clean:webp": "rimraf ./tests/**/*.webp",
    "clean:output": "rimraf ./tests/imageCompression/output",
    "clean:icns": "rimraf ./tests/image2icns/*.icns"
  },
  "dependencies": {
    "@fiahfy/icns": "^0.0.6",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "colors": "^1.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegoptim": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "lodash": "^4.17.15",
    "request": "^2.88.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10"
  }
}
