{
  "name": "@sweet/util-tools",
  "version": "0.0.0-alpha.9",
  "description": "通用工具函数",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development tsc --watch",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:webp && npm run clean:output && cross-env NODE_ENV=test mocha tests/test.js --timeout 180000",
    "clean:webp": "rimraf ./tests/image2webp/image.webp",
    "clean:output": "rimraf ./tests/imageCompression/output"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegoptim": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-webp": "^5.0.0",
    "lodash": "^4.17.11",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">= 9.0.0"
  }
}
