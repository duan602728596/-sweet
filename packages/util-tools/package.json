{
  "name": "@sweet-milktea/util-tools",
  "version": "0.11.2",
  "description": "通用工具函数",
  "homepage": "https://github.com/duan602728596/sweet",
  "repository": {
    "type": "git",
    "url": "git@github.com:duan602728596/sweet.git"
  },
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development tsc --watch",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:test && cross-env NODE_ENV=test mocha test/test.js --timeout 180000",
    "clean:test": "npm run clean:webp && npm run clean:output && npm run clean:icns",
    "clean:webp": "rimraf ./tests/**/*.webp",
    "clean:output": "rimraf ./tests/imageCompression/output",
    "clean:icns": "rimraf ./tests/image2icns/*.icns"
  },
  "dependencies": {
    "@fiahfy/icns": "^0.0.6",
    "chalk": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegoptim": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "lodash": "^4.17.15",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "mocha": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=10"
  }
}
