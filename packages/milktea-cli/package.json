{
  "name": "@sweet-milktea/milktea-cli",
  "version": "0.0.1",
  "description": "sweet cli工具",
  "main": "lib/cli.js",
  "bin": {
    "milktea": "lib/cli.js"
  },
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development tsc --watch",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:test && cross-env NODE_ENV=test mocha ./tests/test.js --timeout 180000",
    "clean:test": "npm run clean:sweet && npm run clean:build",
    "clean:sweet": "rimraf ./tests/.sweet",
    "clean:build": "rimraf ./tests/dist"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "webpack": "^4.34.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-bigint": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@sweet-milktea/milktea": "^0.1.1",
    "@sweet-milktea/server": "^0.0.2",
    "@sweet-milktea/server-log": "^0.0.2",
    "@sweet-milktea/util-tools": "^0.0.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "react": "^16.8.6",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "@sweet-milktea/milktea": "^0.1.1",
    "@sweet-milktea/server": "^0.0.2",
    "@sweet-milktea/server-log": "^0.0.2",
    "@sweet-milktea/util-tools": "^0.0.1"
  },
  "engines": {
    "node": ">= 9.0.0"
  }
}
