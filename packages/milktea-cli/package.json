{
  "name": "@sweet/milktea-cli",
  "version": "0.0.0-alpha.2",
  "main": "lib/cli.js",
  "bin": {
    "milktea": "lib/cli.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run clean && tsc --watch",
    "build": "cross-env NODE_ENV=production npm run clean && tsc",
    "clean": "rimraf ./lib",
    "test": "mocha ./test/test.js --timeout 180000",
    "cleantest": "npm run cleandll && npm run cleanbabelcache && npm run cleanbuild",
    "cleandll": "rimraf ./test/.dll",
    "cleanbabelcache": "rimraf ./test/.babelCache",
    "cleanbuild": "rimraf ./test/build"
  },
  "dependencies": {
    "webpack": "^4.21.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "react": "^16.5.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.3"
  }
}
