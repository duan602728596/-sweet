{
  "name": "@sweet/milktea-cli",
  "version": "0.0.0-alpha.6",
  "main": "lib/cli.js",
  "bin": {
    "milktea": "lib/cli.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run clean && tsc --watch",
    "build": "cross-env NODE_ENV=production npm run clean && tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:test && mocha ./tests/test.js --timeout 180000",
    "clean:test": "npm run clean:dll && npm run clean:babelCache && npm run clean:terserCache && npm run clean:build",
    "clean:dll": "rimraf ./tests/.dll",
    "clean:babelCache": "rimraf ./tests/.babelCache",
    "clean:terserCache": "rimraf ./tests/.terserCache",
    "clean:build": "rimraf ./tests/build"
  },
  "dependencies": {
    "webpack": "^4.29.6",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.0.2",
    "react": "^16.8.4",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools"
  },
  "engines": {
    "node": ">= 9.0.0"
  }
}
