{
  "name": "@sweet/milktea-cli",
  "version": "0.0.0-alpha.9",
  "description": "sweet cli工具",
  "main": "lib/cli.js",
  "bin": {
    "milktea": "lib/cli.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run clean && tsc --watch",
    "build": "cross-env NODE_ENV=production npm run clean && tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:test && mocha ./tests/test.js --timeout 180000",
    "clean:test": "npm run clean:dll && npm run clean:babelCache && npm run clean:terserCache && npm run clean:build",
    "clean:dll": "rimraf ./tests/.dll",
    "clean:babelCache": "rimraf ./tests/.babelCache",
    "clean:terserCache": "rimraf ./tests/.terserCache",
    "clean:build": "rimraf ./tests/build"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "webpack": "^4.31.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "@sweet/server-log": "github:duan602728596/sweet#build-server-log",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "react": "^16.8.6",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "@sweet/server-log": "github:duan602728596/sweet#build-server-log",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools"
  },
  "engines": {
    "node": ">= 9.0.0"
  }
}
