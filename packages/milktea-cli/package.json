{
  "name": "@sweet-milktea/milktea-cli",
  "version": "0.6.3",
  "description": "sweet cli工具",
  "homepage": "https://github.com/duan602728596/sweet",
  "repository": {
    "type": "git",
    "url": "git@github.com:duan602728596/sweet.git"
  },
  "main": "lib/cli.js",
  "bin": {
    "milktea": "lib/cli.js"
  },
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development tsc --watch",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:test && cross-env NODE_ENV=test mocha ./tests/test.js --timeout 180000",
    "clean:test": "npm run clean:sweet && npm run clean:build",
    "clean:sweet": "rimraf ./tests/.sweet",
    "clean:build": "rimraf ./tests/dist"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "webpack": "^5.0.0-beta.1",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-syntax-bigint": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@sweet-milktea/milktea": "^0.6.4",
    "@sweet-milktea/server": "^0.6.4",
    "@sweet-milktea/server-log": "^0.3.9",
    "@sweet-milktea/util-tools": "^0.5.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.2",
    "react": "^16.11.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4"
  },
  "peerDependencies": {
    "@sweet-milktea/milktea": ">=0",
    "@sweet-milktea/server": ">=0",
    "@sweet-milktea/server-log": ">=0",
    "@sweet-milktea/util-tools": ">=0"
  },
  "engines": {
