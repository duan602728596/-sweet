{
  "name": "@sweet/server-log",
  "version": "0.0.0-alpha.9",
  "description": "服务端的koa日志中间件函数",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run clean && tsc --watch",
    "build": "cross-env NODE_ENV=production npm run clean && tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:logs && mocha ./tests/test.js --timeout 180000",
    "clean:logs": "rimraf ./tests/.logs"
  },
  "dependencies": {
    "@log4js-node/logfaces-http": "^1.0.0",
    "koa-log4": "github:duan602728596/koa-log4js#v2.x",
    "log4js": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">= 9.0.0"
  }
}
