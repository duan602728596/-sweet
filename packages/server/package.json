{
  "name": "@sweet-milktea/server",
  "version": "0.3.1",
  "description": "webpack服务和web服务",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development tsc --watch",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./lib",
    "test": "npm run clean:build && cross-env NODE_ENV=test mocha ./tests/test.js --timeout 180000",
    "clean:test": "npm run clean:sweet && npm run clean:build",
    "clean:sweet": "rimraf ./.sweet",
    "clean:build": "rimraf ./tests/dist"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
