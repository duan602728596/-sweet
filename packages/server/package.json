{
  "name": "@sweet/server",
  "version": "0.0.0-alpha.1",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run clean && babel ./src --watch --out-dir=\"./lib\" --config-file=\"../../.babelrc\"",
    "build": "cross-env NODE_ENV=production npm run clean && babel ./src --out-dir=\"./lib\" --config-file=\"../../.babelrc\"",
    "clean": "rimraf ./lib"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "koa-static-cache": "^5.1.2",
    "koa-webpack": "^5.1.0",
    "mime-types": "^2.1.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.2"
  }
